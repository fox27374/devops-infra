apiVersion: traefik.containo.us/v1alpha1
kind: IngressRoute
metadata:
  name: guacamole-ingress
  namespace: guacamole
spec:
  entryPoints:
    - web
  routes:
    - kind: Rule
      match: Host(`{{ guacamole_dns }}`)
      services:
        - kind: Service
          name: guacamole
          port: 80